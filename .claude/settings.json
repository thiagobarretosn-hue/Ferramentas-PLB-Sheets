{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Claude Code settings for Google Apps Script development",
  "enabledPlugins": [
    "agent-sdk-dev@claude-plugins-official"
  ],
  "verbose": true,
  "allowWebSearch": true,
  "allowWebFetch": true,
  "permissions": {
    "allow": [
      "Read",
      "Write",
      "Edit",
      "Glob",
      "Grep",
      "WebSearch",
      "WebFetch"
    ],
    "deny": []
  }
}
